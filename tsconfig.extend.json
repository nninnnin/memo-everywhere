{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@reducers/*": ["src/reducers/*"],
      "@typeDefinition": ["types/index.d.ts"],
      "@typeDefinition/*": ["types/*"]
    },
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["src", "types"]
}
